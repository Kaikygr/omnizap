{
  "source": {
    "include": ["src"],
    "exclude": ["node_modules", "dist"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "encoding": "utf8",
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}
